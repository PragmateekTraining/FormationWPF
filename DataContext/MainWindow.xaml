<Window x:Class="DataContext.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel x:Name="root">
        <!--<StackPanel.DataContext>
            <Binding RelativeSource="{RelativeSource AncestorType=Window}"></Binding>
        </StackPanel.DataContext>-->
        <TextBlock Text="{Binding Text}"></TextBlock>
        <TextBlock Text="{Binding TextClick}"></TextBlock>
        <Grid DataContext="Hello">
            <TextBlock Text="{Binding}"></TextBlock>
        </Grid>
        <Button Click="Button_Click">Go</Button>
    </StackPanel>
</Window>
