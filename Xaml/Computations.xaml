<x:Array xmlns="clr-namespace:Xaml;assembly=Xaml"
		 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"			
		 Type="Result">
    <Result Value="{Calculator 1+1}" />
    <Result Value="{Calculator 1-1}" />
    <Result Value="{Calculator 1*1}" />
    <Result Value="{Calculator 1/1}" />
    <Result Value="{Random Min=-10,Max=+10}" />
    <Result Value="{Random -10,+10}" />
    <Result Value="{Random -10,Max=+10}" />
</x:Array>