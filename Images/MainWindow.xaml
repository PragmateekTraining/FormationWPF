<Window x:Class="Images.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow"
        SizeToContent="WidthAndHeight">
    <StackPanel Orientation="Horizontal">
        <StackPanel.Resources>
            <Style TargetType="Image">
                <Setter Property="Width" Value="48"></Setter>
                <Setter Property="Height" Value="48"></Setter>
                <Setter Property="Margin" Value="2"></Setter>
            </Style>
        </StackPanel.Resources>
        <Image Source="/Assets;component/images/icons/home.png"></Image>
        <Image Source="/Assets;component/images/icons/login.png"></Image>
        <Image Source="/Assets;component/images/icons/open.png"></Image>
        <Image Source="/Assets;component/images/icons/exit.png"></Image>
    </StackPanel>
</Window>
